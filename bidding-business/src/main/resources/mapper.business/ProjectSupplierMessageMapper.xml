<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neteast.business.mapper.ProjectSupplierMessageMapper">

    <resultMap type="com.neteast.business.domain.project.ProjectSupplierMessage" id="SupplierMessageMap">
        <result property="id" column="id" jdbcType="Integer"/>
        <result property="name" column="name" jdbcType="String"/>
        <result property="projectId" column="project_id" jdbcType="Integer"/>
        <result property="recommend" column="recommend" jdbcType="String"/>
        <result property="entryMethod" column="entry_method" jdbcType="String"/>
        <result property="registerTime" column="register_time" jdbcType="Date"/>
        <result property="earnestMoney" column="earnest_money" jdbcType="String"/>
        <result property="createBy" column="create_by" jdbcType="String"/>
        <result property="createTime" column="create_time" jdbcType="Date"/>
        <result property="updateBy" column="update_by" jdbcType="String"/>
        <result property="updateTime" column="update_time" jdbcType="Date"/>
    </resultMap>

    <select id="getList" resultMap="SupplierMessageMap">
        select
            id,project_id,package_id,recommend,name,entry_method,register_time,earnest_money,create_by,create_time,update_by,update_time
        from supplier_message
        <where>
            <if test="name != null and name != ''">
                and name = #{name}
            </if>
            <if test="entryMethod != null and entryMethod != ''">
                and entry_method = #{entryMethod}
            </if>
            <if test="registerTime != null and registerTime != ''">
                and register_time = #{registerTime}
            </if>
            <if test="earnestMoney != null and earnestMoney != ''">
                and earnest_money = #{earnestMoney}
            </if>
            <if test="projectId != null">
                and project_id = #{projectId}
            </if>
            <if test="packageId != null">
                and package_id = #{packageId}
            </if>
            <if test="recommend != null and recommend != ''">
                and recommend = #{recommend}
            </if>
        </where>
    </select>



</mapper>