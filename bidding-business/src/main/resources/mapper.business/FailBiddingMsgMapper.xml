<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neteast.business.mapper.FailBiddingMsgMapper">

    <resultMap type=".entity.FailBiddingMessage" id="FailBiddingMessageMap">
        <result property="id" column="id" jdbcType="Integer"/>
        <result property="extProjectId" column="ext_project_id" jdbcType="Integer"/>
        <result property="failReason" column="fail_reason" jdbcType="String"/>
        <result property="projectCode" column="project_code" jdbcType="String"/>
        <result property="filingsNumber" column="filings_number" jdbcType="String"/>
        <result property="projectName" column="project_name" jdbcType="String"/>
        <result property="procureType" column="procure_type" jdbcType="String"/>
        <result property="createBy" column="create_by" jdbcType="String"/>
        <result property="createTime" column="create_time" jdbcType="Date"/>
        <result property="updateBy" column="update_by" jdbcType="String"/>
        <result property="updateTime" column="update_time" jdbcType="Date"/>
    </resultMap>

    <select id="getList" resultMap="FailBiddingMessageMap">
        select m.id,m.ext_project_id,m.fail_reason,project_code,filings_number,project_name,procure_type,m.create_by,m.create_time,m.update_by,m.update_time from fail_bidding_message m
            LEFT JOIN project_message on ext_project_id = project_message.id
        <where>
            <if test="id != null and id != ''">
                and id = #{id}
            </if>
            <if test="extProjectId != null and extProjectId != ''">
                and ext_project_id = #{extProjectId}
            </if>
        </where>
    </select>

</mapper>